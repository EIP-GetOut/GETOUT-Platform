{
  "name": "getout-backend",
  "description": "Backend for Getout mobile application.",
  "author": {
    "name": "Alexandre Chetrit",
    "url": "https://github.com/chetrit/"
  },
  "license": "UNLICENSED",
  "private": true,
  "nodemonConfig": {
    "watch": [
      "./**/*.ts"
    ],
    "events": {
      "start": "clear"
    },
    "legacyWatch": true
  },
  "main": "./app.ts",
  "scripts": {
    "start": "nodemon -r tsconfig-paths/register ./src/app.ts",
    "test": "NODE_ENV=test jest --silent",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate:generate": "npm run typeorm migration:generate -c main -- -n",
    "migrate:up": "npm run typeorm migration:run -c main",
    "migrate:down": "npm run typeorm migration:revert -c main"
  },
  "engines": {
    "node": ">=14.17.*",
    "npm": ">=7.*"
  },
  "dependencies": {
    "@getbrevo/brevo": "^2.1.1",
    "@googleapis/books": "^2.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/python-shell": "^1.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "eslint-plugin-sort-exports": "^0.9.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.7.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "moviedb-promise": "^4.0.3",
    "nodemailer": "^6.9.13",
    "python-shell": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.18.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "connect-redis": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "express-session": "^1.18.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.3"
  },
  "version": "0.5.5"
}
